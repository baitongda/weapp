/* pages/search/search.wxss */
page {
  background-color: #fff;
}
.nav-search {
  position: fixed;
  top: 0;
  left: 0;
  z-index: 10;
  width: 100%;
}
.searchword{
  margin-top: 10px;
  padding: 10px 10px;
  font-size: 16px;
}
.clear{
  float: right;
}
.keyword{ 
  float: left;
  max-width: 50%;
  padding: 2px 5px;
  margin: 5px 5px;
  border: 2px solid #f2f2f2;
  border-radius:5%;
  overflow: hidden;
  display:block;
  white-space:nowrap; 
  text-overflow: ellipsis;
}
